{
  "name": "Appjusto Admin",
  "description": "Extension to avoid auto discard in Appjusto admin tabs",
  "version": "1.0",
  "icons": {
    "16":"icon.png",
    "48":"icon.png",
    "128":"icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "webNavigation"],
  "manifest_version": 3
}